{
	"name": "cdk-lambda-urls1",
	"version": "0.1.0",
	"bin": {
		"cdk-lambda-urls1": "bin/cdk-lambda-urls1.js"
	},
	"scripts": {
		"build": "tsc",
		"watch": "tsc -w",
		"test": "jest",
		"cdk": "cdk",
		"deploy": ". .env && cdk deploy -O var/outputs.json && node scripts/cdk_outputs_to_shell.mjs",
		"destroy": ". .env && cdk destroy",
		"format": "biome format --write .",
		"format:full": "biome format --write . && dprint fmt && textlint --fix \"**/*.md\"",
		"hello": ". var/outputs.sh && curl \"${CDK_CDKLAMBDAURLS1STACK_LAMBDA1FUNCTIONURL}hello\""
	},
	"devDependencies": {
		"@types/express": "^5.0.3",
		"@types/jest": "^30.0.0",
		"@types/node": "24.8.1",
		"aws-cdk": "2.1030.0",
		"jest": "^30.2.0",
		"ts-jest": "^29.4.5",
		"ts-node": "^10.9.2",
		"typescript": "~5.9.3"
	},
	"dependencies": {
		"@vendia/serverless-express": "^4.12.6",
		"aws-cdk-lib": "2.220.0",
		"constructs": "^10.4.2",
		"express": "^5.1.0"
	}
}
