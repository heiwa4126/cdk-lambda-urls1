{
	"name": "cdk-lambda-urls1",
	"version": "0.1.0",
	"bin": {
		"cdk-lambda-urls1": "bin/cdk-lambda-urls1.js"
	},
	"scripts": {
		"build": "tsc",
		"watch": "tsc -w",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:coverage": "vitest run --coverage",
		"synth": "cdk synth",
		"deploy": ". .env && cdk deploy -O var/outputs.json && cdkoutputs2sh -v",
		"diff": ". .env && cdk diff",
		"destroy": ". .env && cdk destroy",
		"format": "biome format --write .",
		"format:full": "biome format --write . && dprint fmt && textlint --fix \"**/*.md\"",
		"hello": ". var/outputs.sh && curl \"${CDK_CDKLAMBDAURLS1STACK_LAMBDA1FUNCTIONURL}hello\"",
		"security": "trivy fs --skip-dirs node_modules,cdk.out . && trivy config --skip-dirs node_modules,cdk.out .",
		"audit": "pnpm audit && trivy fs --scanners vuln package.json"
	},
	"devDependencies": {
		"@types/node": "24.9.1",
		"aws-cdk": "2.1031.0",
		"cdkoutputs2sh": "^0.0.2",
		"ts-node": "^10.9.2",
		"typescript": "~5.9.3",
		"vitest": "^4.0.4"
	},
	"dependencies": {
		"aws-cdk-lib": "2.221.0",
		"constructs": "^10.4.2",
		"hono": "^4.10.4"
	}
}
